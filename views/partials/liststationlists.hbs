{{#each stationlist}}
  <section class="ui stacked segment">
    <h2 class="ui header">
      {{title}}
    </h2>
    {{>liststationlists}}
    {{#each stationlist}}
      {{liststationlists
        date=date
        latitude=latitude
        longitude=longitude
        code=code
        temp=temp
        windSpeed=windSpeed
        windDirection=windDirection
        pressure=pressure
        icon=icon
        stationlistid=../stationlist.id}}
    {{/each}}
    <a href="/stationlist/{{id}}" class="ui icon button">
      <i class="icon green folder open"></i>
    </a>
    <a href="/dashboard/deletestationlist/{{id}}" class="ui icon button">
      <i class="icon red trash"></i>
    </a>
  </section>
{{/each}}